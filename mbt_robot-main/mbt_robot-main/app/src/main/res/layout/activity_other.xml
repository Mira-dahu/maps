<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".Other">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/black"


        app:title="    返回"

        app:titleMarginStart="10dp"
        app:titleTextColor="@color/white"></androidx.appcompat.widget.Toolbar>

    <Button
        android:id="@+id/hiddenBack12"
        android:layout_width="wrap_content"
        android:layout_height="?attr/actionBarSize"
        android:alpha="0"
        android:onClick="destroyTheActivity"
        android:text="Button"
        android:visibility="visible"
        tools:visibility="visible" />

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="375dp"
        android:background="@drawable/ba"
        android:onClick="destroyTheActivity"></Button>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-2dp"
        android:textStyle="bold"

        android:layout_marginTop="59dp"
        android:text="  其他"
        android:textColor="@color/white"
        android:textSize="45dp">
    </TextView>

    <Button
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/radius_up"
        android:gravity="center_vertical|left"
        android:onClick="editMapNameTextView"
        android:stateListAnimator="@null"
        android:text="      地图名称"
        android:textColor="@color/black"
        android:textSize="18dp"></Button>

    <Button
        android:stateListAnimator="@null"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="30dp"
        android:gravity="center_vertical|left"
        android:background="@color/white"
        android:text="      创建时间"
        android:textColor="@color/black"
        android:textSize="18dp"></Button>

    <View
        android:layout_width="250dp"
        android:layout_height="3px"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="30dp"
        android:background="@color/grey">
    </View>

    <Button
        android:stateListAnimator="@null"
        android:layout_width="350dp"
        android:layout_height="51dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/radius_down"
        android:text="      面积大小"
        android:gravity="center_vertical|left"
        android:textColor="@color/black"
        android:textSize="18dp"></Button>

    <View
        android:layout_width="250dp"
        android:layout_height="3px"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="30dp"
        android:background="@color/grey">
    </View>

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="40dp"
        android:src="@mipmap/img">

    </ImageView>

    <TextView
        android:id="@+id/name_view"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="60dp"
        android:gravity="center_vertical|right"
        android:text="地图1名字"
        android:textSize="18dp">

    </TextView>

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="60dp"
        android:text="2023年3月17日  10:17"
        android:textSize="18dp"
        android:gravity="center_vertical|right"
        >

    </TextView>

    <TextView
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="60dp"
        android:text="75 m2"
        android:textSize="18dp"
        android:gravity="center_vertical|right"
        >

    </TextView>

    <Button

        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="30dp"

        android:background="@drawable/radius_border"
        android:text="恢复初始状态"
        android:textColor="@color/red"
        android:textSize="22dp"
        android:textStyle="bold" />
    <TextView
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="555dp"

        android:layout_marginEnd="30dp"
        android:text="恢复初始状态后，地图的一切编辑改动将全部删除"
        android:textColor="@color/grey"
        android:textSize="10dp"></TextView>

    <Button

        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="30dp"

        android:background="@drawable/radius_border"
        android:text="删除地图"
        android:textColor="@color/red"
        android:textSize="22dp"
        android:textStyle="bold" />


</RelativeLayout>